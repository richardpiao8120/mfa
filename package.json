{
  "name": "mfa-root",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel start:host start:remoteA start:remoteB start:remoteC start:remoteD",
    "start:host": "cd host && npm start",
    "start:remoteA": "cd remoteA && npm start",
    "start:remoteB": "cd remoteB && npm start",
    "start:remoteC": "cd remoteC && npm start",
    "start:remoteD": "cd remoteD && npm start",
    "setup": "npm-run-all setup:host setup:remoteA setup:remoteB setup:remoteC setup:remoteD",
    "setup:host": "cd host && npm install",
    "setup:remoteA": "cd remoteA && npm install",
    "setup:remoteB": "cd remoteB && npm install",
    "setup:remoteC": "cd remoteC && npm install",
    "setup:remoteD": "cd remoteD && npm install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "antd": "^5.15.2",
    "ag-grid-community": "^31.2.1",
    "ag-grid-react": "^31.2.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
